<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomButtonStyle">
        <item name="android:background">@drawable/rounded_button</item>
        <item name="android:textColor">#000000</item> <!-- 검정색 텍스트 -->
        <item name="android:padding">8dp</item>
    </style>
    <style name="alarm_toggle">
        <item name="thumbTint">@color/white</item>
        <item name="track">@drawable/style_toggle_btn</item>
        <item name="android:thumb">@drawable/style_toggle_thumb</item>
        <item name="android:state_checked">true</item>
    </style>

    <style name="custom_chip_style">
        <item name="backgroundTint">@color/white</item>

    </style>

    <style name="CustomChipStyle" parent="Widget.MaterialComponents.Chip.Action">
        <item name="rippleColor">@android:color/transparent</item>
        <item name="android:stateListAnimator">@null</item>
    </style>
</resources>